<div id="index-banner" class="parallax-container">
  <div class="section no-pad-bot">
    <div class="container">
      <br><br>
      <h1 class="header center teal-text text-lighten-2">Paws and Pooches Animal Shelter</h1>
      <div class="row center">
        <h5 class="header col s12 light">A modern responsive front-end framework based on Material Design</h5>
      </div>
      <div class="row center">
        <a href="http://materializecss.com/getting-started.html" id="download-button" class="btn-large waves-effect waves-light teal lighten-1">Get Started</a>
      </div>
      <h3>Pets:</h3>
      <ul id="example-list" class="list-group">
        {{#each pets}}
        <li data-id="{{this.id}}" class="list-group-item">
          <a href="example/{{this.id}}">{{this.name}}</a>
          <br>
          <a href="example/{{this.id}}">{{this.breed}}</a>
          <button class="btn btn-danger float-right delete">ï½˜</button>
        </li>
        {{/each}}
      </ul>
    </div>
  </div>
  <div class="parallax"><img src="background1.jpg" alt="Unsplashed background img 1"></div>
</div>